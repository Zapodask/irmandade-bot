{
    "env": {
        "node": true,
        "es2021": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "quotes": [1, "single"],
        "brace-style": "warn",
        "semi":"warn",
        "camelcase":"warn",
        "no-duplicate-imports":"warn",
        "no-extra-semi":"warn"
    }
}
